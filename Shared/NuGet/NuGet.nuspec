<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.OxyPlot</id>
    <version>1.1.100</version>
    <title>OxyPlot Charts for Zebble (Xamarin)</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.OxyPlot</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.OxyPlot/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright © 2018 Geeks Ltd - All rights reserved.</copyright>
    <description>A Zebble plugin to draw OxyPlot charts.</description>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="OxyPlot.Xamarin.Android" version="1.0.0" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="OxyPlot.Xamarin.iOS" version="1.0.0" />
      </group>
      <group targetFramework="UAP10">
        <dependency id="OxyPlot.Windows" version="1.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.OxyPlot.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.OxyPlot.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.OxyPlot.UWP.*" target="lib\UAP10\" />
  </files>
</package>